#PRINT INTEGERS
number=int(input())
counter=1
while counter<=number:
    print(counter)
    counter=counter+1
    
INPUT:
3
OUTPUT:
1
2
3
------------------------------------------
#PRINT INTEGERS FROM M TO N
M = int(input())
N = int(input())
counter=M
while  counter<=N:
    print(counter )
    counter=counter+1
    
INPUT:
   2
   6
OUTPUT:
   2
   3
   4
   5
   6
 -------------------------------------------
 #PRINT INTEGERS
 N=int(input())
counter=0
while counter<=N:
    N=N-1
    print(N+1)
    counter=counter+1
    
print(N)
 
 INPUT:
    5
 OUTPUT:
    5
    4
    3
    2
    1
--------------------------------------------

   



